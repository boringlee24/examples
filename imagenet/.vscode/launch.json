    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${fileDirname}",
            "env": {
                "CUDA_VISIBLE_DEVICES": "0,1,2,3"
            },        
            "args": ["-a", "resnet50", "-b", "64", "--multiprocessing-distributed", 
                    "--world-size", "1", "--rank", "0", 
                    "--benchmarking", 
                    "--gpu-type", "v100", "--num-gpu", "4", "--iter-limit", "800",
                    "imagenet"]
        }
    ]
}